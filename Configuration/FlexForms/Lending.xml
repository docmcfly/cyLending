<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.title
					</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.purposes>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.purposes
							</label>
							<config>
								<type>text</type>
								<cols>40</cols>
                                <rows>5</rows>
							</config>
						</TCEforms>
					</settings.purposes>
					<settings.usualLendingTermHours>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.usualLendingTermHours
							</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<range>
									<lower>0</lower>
									<upper>168</upper>
								</range>
								<default>3</default>
							</config>
						</TCEforms>
					</settings.usualLendingTermHours>
					<settings.usualLendingTermMinutes>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.usualLendingTermMinutes
							</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<range>
									<lower>0</lower>
									<upper>59</upper>
								</range>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.usualLendingTermMinutes>
					<settings.frontendUserStroageUids>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.frontendUserStroageUids
							</label>
							<config>
								<type>select</type>
								<renderType>selectTree</renderType>
								<foreign_table>pages</foreign_table>
								<foreign_table_where>ORDER BY pages.sorting</foreign_table_where>
								<size>20</size>
								<treeConfig>
									<parentField>pid</parentField>
									<appearance>
										<maxLevels>7</maxLevels>
									</appearance>
								</treeConfig>
								<minitems>1</minitems>
							</config>
						</TCEforms>
					</settings.frontendUserStroageUids>
					<settings.lendingStorageUids>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.lendingStorageUids
							</label>
							<config>
								<type>select</type>
								<renderType>selectTree</renderType>
								<foreign_table>pages</foreign_table>
								<foreign_table_where>ORDER BY pages.sorting</foreign_table_where>
								<size>20</size>
								<treeConfig>
									<parentField>pid</parentField>
									<appearance>
										<maxLevels>7</maxLevels>
									</appearance>
								</treeConfig>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.lendingStorageUids>
					<settings.otherLendingStorageUids>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.otherLendingStorageUids
							</label>
							<config>
								<type>select</type>
								<renderType>selectTree</renderType>
								<foreign_table>pages</foreign_table>
								<foreign_table_where>ORDER BY pages.sorting</foreign_table_where>
								<size>20</size>
								<treeConfig>
									<parentField>pid</parentField>
									<appearance>
										<maxLevels>7</maxLevels>
									</appearance>
								</treeConfig>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.otherLendingStorageUids>
					<settings.lendingObjectStorageUids>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.lendingObjectStorageUids
							</label>
							<config>
								<type>select</type>
								<renderType>selectTree</renderType>
								<foreign_table>pages</foreign_table>
								<foreign_table_where>ORDER BY pages.sorting</foreign_table_where>
								<size>20</size>
								<treeConfig>
									<parentField>pid</parentField>
									<appearance>
										<maxLevels>7</maxLevels>
									</appearance>
								</treeConfig>
								<minitems>1</minitems>
							</config>
						</TCEforms>
					</settings.lendingObjectStorageUids>
					<settings.reasonsForPreventionContentElement>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.reasonsForPreventionContentElement
							</label>
							<config>
								<type>input</type>
								<renderType>inputLink</renderType>
								<eval>
									trim
								</eval>
								<fieldControl>
									<linkPopup>
										<options>
											<blindLinkOptions>file,folder,mail,telephone,url</blindLinkOptions>
										</options>
									</linkPopup>
								</fieldControl>
							</config>
						</TCEforms>
					</settings.reasonsForPreventionContentElement>
					<settings.reasonsForPreventionTSPageType>
						<TCEforms>
							<label>LLL:EXT:cy_lending/Resources/Private/Language/locallang_be_lending.xlf:flexforms_general.settings.reasonsForPreventionTSPageType
							</label>
							<config>
								<type>input</type>
								<min>1</min>
							</config>
						</TCEforms>
					</settings.reasonsForPreventionTSPageType>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
